{
  "name": "@duplojs/duplojs",
  "version": "0.0.0",
  "author": "mathcovax",
  "description": "",
  "repository": "https://github.com/duplojs/duplojs",
  "license": "ISC",
  "keywords": [],
  "types": "./types/index.ts",
  "exports": {
    "default": {
      "import": "./dist/duplo.mjs",
      "require": "./dist/duplo.cjs"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nodemon --exec 'npm run test:speed'",
    "build": "rollup --config && npx tsc",
    "test": "npm run test:e2e",
    "test:speed": "npm run test:e2e:speed",
    "test:e2e": "ts-node test/E2E",
    "test:tu": "vitest --config test/TU/vitest.config.mts",
    "test:tu:watch": "vitest --config test/TU/vitest.config.mts --watch",
    "test:tu:coverage": "vitest --config test/TU/vitest.config.mts --coverage",
    "test:e2e:speed": "ts-node --transpiler sucrase/ts-node-plugin test/E2E --speed"
  },
  "dependencies": {
    "fast-querystring": "^1.1.2",
    "mime": "^3.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@duplojs/worker-testing": "^0.2.5",
    "@types/mime": "^3.0.1",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-istanbul": "^1.2.1",
    "autocannon": "^7.11.0",
    "eslint": "^8.45.0",
    "mock-fs": "^5.2.0",
    "node-mocks-http": "^1.14.1",
    "nodemon": "^3.0.1",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "sucrase": "^3.34.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vitest": "^1.2.1"
  },
  "nodemonConfig": {
    "watch": [
      "scripts",
      "test"
    ],
    "ext": "ts, js"
  }
}
